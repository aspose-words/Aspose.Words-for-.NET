@model ViewModel

@{
    Layout = "~/Views/Shared/Layout.cshtml";
}

@Html.Partial("Main")

@Html.Partial("FooterProduct", Model)

@section scripts {
    @Scripts.Render("~/scripts/shared/metadata.js")

    <script type="text/javascript">
        var o = @Html.Raw(Model.JSOptions);
        $.extend(o, {
            Method: requestMetadata
        });
    </script>
}